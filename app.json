{
  "expo": {
    "scheme": "myapp",
    "name": "Koruja",
    "slug": "koruja",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/korujaIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/AttlasBlack.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B2438"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/korujaIcon.png",
        "backgroundColor": "#0B2438"
      },
      "package": "com.gustavomv.koruja",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Poppins-Regular.ttf"
          ]
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Permita $(PRODUCT_NAME) acessar suas fotos.",
          "savePhotosPermission": "Permita $(PRODUCT_NAME) salvar fotos no dispositivo.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Permita $(PRODUCT_NAME) acessar sua c√¢mera",
          "microphonePermission": "Permita $(PRODUCT_NAME) acessar seu microfone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6154723c-1c1d-4ce0-b5a6-7d1488f650a8"
      }
    }
  }
}
