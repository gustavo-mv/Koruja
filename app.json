{
  "expo": {
    "scheme": "myapp",
    "name": "attlasAssistente",
    "slug": "attlasAssistente",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/attlasAssistente.png",
    "userInterfaceStyle": "light",

    "splash": {
      "image": "./assets/AttlasBlack.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Poppins-Regular.ttf"]
        }
      ]
    ]
  }
}
