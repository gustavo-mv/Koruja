{
  "expo": {
    "scheme": "myapp",
    "name": "Attlas Assistente",
    "slug": "attlasAssistente",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/attlasAssistente.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/AttlasBlack.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/AttlasIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gustavomv.attlasAssistente"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Poppins-Regular.ttf"]
        }
      ],

      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ec03f813-6c5f-4830-874b-d4ba8f6e27f4"
      }
    }
  }
}
